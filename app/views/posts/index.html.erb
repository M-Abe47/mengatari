<table>
  <thead>

  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td>
          <%= link_to post_path(post.id) do %>
            <%= attachment_image_tag post, :image %>
          <% end %>
        </td>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        <td><%= post.user.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>