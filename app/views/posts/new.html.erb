<h1>投稿フォーム</h1>
<%= form_with model: @post, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>タグ</h4>
  <%= f.label :tag_list %>
  <%= f.text_field :tag_list, value: @post.tag_list.join(','), class: "form-control" %>
  <h4>説明</h4>
  <%= f.text_area :body %>
  <%= f.submit '投稿' %>
<% end %>